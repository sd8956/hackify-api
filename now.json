{
  "name": "hackify-api",
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "MONGO_URI": "@hackify_mongo_uri",
    "JWT_SECRET": "@jwt_secret",
    "JWT_EXPIRES_IN": "@jwt_expires_in",
    "S3_SECRET_ACCESS_KEY": "@s3_secret_acces_key",
    "S3_ACCESS_KEY_ID": "@s3_acces_key_id",
    "S3_BUCKET": "@s3_bucket"
  }
}